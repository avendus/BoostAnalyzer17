  select MR >= 800
  select R2_1vl >= 0.08
  select muon_veto.size + electron_veto.size = 1
  select bjets_loose.size = 0
  select WMassTag.size >= 1
  select dphi < 2.8
  select 30 <= MT_vetoLep < 100

region W_cat_Z
