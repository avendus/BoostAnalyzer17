Region VetoLepton				# Hadronic Seven-jet
   select Hadronicpreselection
   select MR > 650
   select R2 > 0.3
   select btag.size >= 3
   select vetoLeptons.size = 1
   select PT > 5 
   select 30 < MT < 100
   select AK4Jet >= 7
   select tau_veto.size = 0
   

Region VetoTau					# Hadronic Seven-jet
   select Hadronicpreselection
   select MR > 650
   select R2 > 0.3
   select btag.size >= 3
   select LooseHadronictau.size = 1
   select PT > 20
   select 30 < MT < 100
   select AK4Jet >= 7
   select tau_veto.size = 0
   

Region one-Lepton-Invisible			# Hadronic Seven-jet
   select Hadronicpreselection
   select MR > 650
   select R2_1vl > 0.3
   select btag.size >= 3
   select TightLeptons.size = 1
   select MT_vetolep>=30 && MT_vetolep<100
   select AK4Jet >= 7
   select tau_veto.size = 0


Region Dilepton-Invisible			# Hadronic Seven-jet
   select Hadronicpreselection
   select MR > 650
   select R2_ll > 0.3
   select btag.size >= 3
   select TightLeptons.size = 2 
   select btag.size = 0 
   select 80 < M_ll < 110 
   select AK4Jet >= 7
   select tau_veto.size = 0


Region Photon-jet-Invisible			# Hadronic Seven-jet
   select Hadronicpreselection
   select MR_pho > 650
   select R2_pho > 0.3
   select LoosePhoton.size = 1 
   select PT > 185 
   select 80 < M_ll < 110 
   select AK4Jet >= 7
   select tau_veto.size = 0


Region QCD					# Hadronic Seven-jet
   select Hadronicpreselection
   select dphi < 2.8 
   select MR > 650
   select R2 > 0.3
   select btag.size >= 3
   select AK4Jet >= 7
   select tau_veto.size = 0




